{
  "name": "@itwin/itwinui-illustrations-react",
  "version": "1.0.2",
  "description": "Exports svgs from @itwin/itwinui-illustrations as React Components",
  "author": "Bentley Systems",
  "license": "MIT",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "cjs/index.d.ts",
  "files": [
    "cjs/**/*",
    "esm/**/*"
  ],
  "scripts": {
    "build": "yarn illustrations:gen && yarn copyright && yarn build:esm && yarn build:cjs",
    "build:local": "yarn clean && yarn build",
    "build:esm": "tsc -p esm.tsconfig.json",
    "build:cjs": "tsc -p cjs.tsconfig.json",
    "clean": "rimraf cjs && rimraf esm && rimraf src/illustrations",
    "illustrations:gen": "svgr --typescript --prettier-config ../../.prettierrc ../illustrations/illustrations --out-dir src/illustrations --index-template ../../svgr-index-template.js --no-svgo",
    "copyright": "node ../../scripts/copyright-linter.js"
  },
  "devDependencies": {
    "@svgr/cli": "^5.5.0",
    "prettier": "^1.19.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rimraf": "^2.6.2",
    "typescript": "~4.1.5"
  },
  "peerDependencies": {
    "react": "^16.8.6 || ^17.0.0",
    "react-dom": "^16.8.6 || ^17.0.0"
  }
}
